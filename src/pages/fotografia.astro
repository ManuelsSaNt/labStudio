---
import Nav from "../components/Nav.astro";
import IndexLayout from "../layouts/IndexLayout.astro";

// images
import Cofee1 from "../img/labstudio/design/coffe-bran-cups.png";
import Cofee2 from "../img/labstudio/design/coffe-cup-min.jpeg";
import Cofee3 from "../img/labstudio/design/coffe-red-1.png";
import Cofee4 from "../img/labstudio/design/red-coffe-bag.jpeg";
import Footer from "../components/Footer.astro";
---

<IndexLayout
  title="LABSTUDIO | Fotografia"
  ogTitle="LASBSTUDIO | Fotografia"
  description=""
  ogDescription=""
  keywords=""
>
  <Nav />
  <div class="container">
    <div class="row justify-content-between">
      <div
        class="col-12 col-md-4 position-md-sticky d-flex align-items-center vh-md-100"
        style="top: 0;"
      >
        <!-- Sidebar -->
        <aside class="py-4 py-md-6 position-relative" style="top: -100px">
          <div class="img-fluid mb-4 text-body">
            <h1>Fotografia</h1>
          </div>
          <p class="fs-lg mb-7 text-body-secondary">
            We created a photographic brand for Larq thatâ€™s fun and exciting.
          </p>

          <ul class="list-group list-group-flush">
            <li class="list-group-item">
              <h6 class="d-flex mb-0 text-uppercase">
                Year <span class="ms-auto">2019</span>
              </h6>
            </li>
            <li class="list-group-item">
              <h6 class="d-flex mb-0 text-uppercase">
                Service <span class="ms-auto">Photography</span>
              </h6>
            </li>
            <li class="list-group-item">
              <h6 class="d-flex mb-0 text-uppercase">
                Timeline <span class="ms-auto">60 Days</span>
              </h6>
            </li>
          </ul>
        </aside>
      </div>
      <div class="col-12 col-md-7">
        <section class="pb-8 pt-md-8 pb-md-8">
          <a
            class="d-block"
            data-bigpicture={'{"imgSrc":"' + Cofee2.src + '"}'}
            href="#"
          >
            <img class="img-fluid rounded" src={Cofee2.src} alt="..." />
          </a>
          <a
            class="d-block mt-7"
            data-bigpicture={'{"imgSrc":"' + Cofee1.src + '"}'}
            href="#"
          >
            <img class="img-fluid rounded" src={Cofee1.src} alt="..." />
          </a>
          <a
            class="d-block mt-7"
            data-bigpicture={'{"imgSrc":"' + Cofee3.src + '"}'}
            href="#"
          >
            <img class="img-fluid rounded" src={Cofee3.src} alt="..." />
          </a>
          <a
            class="d-block mt-7"
            data-bigpicture={'{"imgSrc":"' + Cofee4.src + '"}'}
            href="#"
          >
            <img class="img-fluid rounded" src={Cofee4.src} alt="..." />
          </a>
          <a
            class="d-block mt-7"
            data-bigpicture='{"imgSrc": "assets/img/portfolio/portfolio-25.jpg"}'
            href="#"
          >
            <img
              class="img-fluid rounded"
              src="assets/img/portfolio/portfolio-25.jpg"
              alt="..."
            />
          </a>
        </section>
      </div>
    </div>
  </div>

  <!-- SHAPE -->
  <div class="position-relative">
    <div class="shape shape-bottom shape-fluid-x text-light">
      <svg viewBox="0 0 2880 48" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M0 48h2880V0h-720C1442.5 52 720 0 720 0H0v48z" fill="currentColor"></path></svg>
    </div>
  </div>

  <section class="pt-7 pb-10 py-md-10 bg-light">
    <div class="container">
      <div class="row align-items-center mb-5">
        <div class="col-12 col-md">

          <h3 class="mb-0">Related Work</h3>

          <p class="mb-0 text-body-secondary">
            Other goodies that are similar to this one.
          </p>
        </div>
        <div class="col-12 col-md-auto">
          <a
            href="#!"
            class="btn btn-sm btn-outline-gray-300 d-none d-md-inline"
          >
            View all
          </a>
        </div>
      </div>
      <div class="row">
        <div class="col-12 col-md-6 col-lg-4 d-flex">

          <a class="card mb-6 mb-lg-0 shadow-light-lg" href="#!">
            <div class="card-zoom">
              <img
                class="card-img-top"
                src="assets/img/portfolio/portfolio-1.jpg"
                alt="..."
              />
            </div>
            <div class="card-body">
              <div class="shape shape-bottom-100 shape-fluid-x text-white">
                <svg viewBox="0 0 2880 48" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M0 48h2880V0h-720C1442.5 52 720 0 720 0H0v48z" fill="currentColor"></path></svg>
              </div>
              <h6 class="text-uppercase mb-1 text-body-secondary">Branding</h6>
              <h4 class="mb-0">Curology Campaign</h4>
            </div>
          </a>
        </div>
        <div class="col-12 col-md-6 col-lg-4 d-flex">
          <!-- Card -->
          <a class="card mb-6 mb-lg-0 shadow-light-lg" href="#!">
            <!-- Image -->
            <div class="card-zoom">
              <img
                class="card-img-top"
                src="assets/img/portfolio/portfolio-2.jpg"
                alt="..."
              />
            </div>

            <!-- Body -->
            <div class="card-body">
              <!-- Shape -->
              <div class="shape shape-bottom-100 shape-fluid-x text-white">
                <svg
                  viewBox="0 0 2880 48"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg"
                  ><path
                    d="M0 48h2880V0h-720C1442.5 52 720 0 720 0H0v48z"
                    fill="currentColor"></path></svg
                >
              </div>

              <!-- Preheading -->
              <h6 class="text-uppercase mb-1 text-body-secondary">Branding</h6>

              <!-- Heading -->
              <h4 class="mb-0">Honest Packaging</h4>
            </div>
          </a>
        </div>
        <div class="col-12 col-md-6 col-lg-4 d-flex">
          <!-- Card -->
          <a class="card d-md-none d-lg-flex shadow-light-lg" href="#!">
            <!-- Image -->
            <div class="card-zoom">
              <img
                class="card-img-top"
                src="assets/img/portfolio/portfolio-4.jpg"
                alt="..."
              />
            </div>

            <!-- Body -->
            <div class="card-body">
              <!-- Shape -->
              <div class="shape shape-bottom-100 shape-fluid-x text-white">
                <svg
                  viewBox="0 0 2880 48"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg"
                  ><path
                    d="M0 48h2880V0h-720C1442.5 52 720 0 720 0H0v48z"
                    fill="currentColor"></path></svg
                >
              </div>

              <!-- Preheading -->
              <h6 class="text-uppercase mb-1 text-body-secondary">Ideation</h6>

              <!-- Heading -->
              <h4 class="mb-0">Doodle Pad</h4>
            </div>
          </a>
        </div>
      </div>
      <!-- / .row -->
    </div>
    <!-- / .container -->
  </section>

  <Footer useMap={false} />
</IndexLayout>
