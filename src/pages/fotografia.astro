---
import Nav from "../components/Nav.astro";
import IndexLayout from "../layouts/IndexLayout.astro";

// images
import Cofee1 from "../img/labstudio/design/coffe-bran-cups.png";
import Cofee2 from "../img/labstudio/design/coffe-cup-min.jpeg";
import Cofee3 from "../img/labstudio/design/coffe-red-1.png";
import Cofee4 from "../img/labstudio/design/red-coffe-bag.jpeg";
import Footer from "../components/Footer.astro";
import PhotographyCard from "../components/PhotographyCard.astro";
import WhatsApp from "../components/WhatsApp.astro";
---

<IndexLayout
  title="LABSTUDIO | Fotografia"
  ogTitle="LASBSTUDIO | Fotografia"
  description="Descubre nuestro mundo de Fotografía profesional en Los Mochis, Sinaloa. Desde retratos hasta fotografía de productos, en Labstudio Media capturamos momentos con precisión y creatividad para destacar tu marca."
  ogDescription="Descubre la excelencia en fotografía con Labstudio Media. Momentos capturados con precisión y creatividad para destacar tu marca. Desde retratos profesionales hasta fotografía de productos, nuestro equipo te brinda imágenes que cuentan historias."
  keywords="Fotografía profesional, Fotografía en Los Mochis, Fotografía en Sinaloa, Fotógrafo profesional, Estudio fotográfico, Fotografía de retratos, Fotografía comercial, Sesiones fotográficas, Fotografía creativa, Fotografía artística, Fotografía de eventos, Fotografía de productos, Fotografía México"
>
  <Nav />
  <div class="container">
    <div class="row justify-content-between">
      <div
        class="col-12 col-md-4 position-md-sticky d-flex align-items-center vh-md-100"
        style="top: 0;"
      >
        <aside class="py-4 py-md-6 position-relative" style="top: -100px"  data-aos="fade-right">
          <div class="img-fluid mb-4 text-body">
            <h1>Fotografia</h1>
          </div>
          <p class="fs-lg mb-7 text-body-secondary">
            Descubre la excelencia en fotografía con Labstudio. Momentos
            capturados con precisión y creatividad para destacar tu marca.
          </p>
          <a
            href="https://wa.me/526688257868?text=Hola%20LabStudio%2C%20quiero%20cotizar%20un%20paquete%20de%20fotograf%C3%ADa.%20%C2%BFPodr%C3%ADan%20ayudarme%20con%20eso%3F%F0%9F%93%B7"
            target="_blank"
            class="btn btn-primary-soft lift lab-back-color button-knowus text-light w-75"
          >
            Cotiza Ahora
            <svg
              xmlns="http://www.w3.org/2000/svg"
              fill="#f9fbfd"
              viewBox="0 0 24 24"
              width="22px"
              height="22px"
              class="position-relative"
              style="bottom: 2px; left: 5px;"
              ><path
                d="M19,4h-.508L16.308,1.168A3.023,3.023,0,0,0,13.932,0H10.068A3.023,3.023,0,0,0,7.692,1.168L5.508,4H5A5.006,5.006,0,0,0,0,9V19a5.006,5.006,0,0,0,5,5H19a5.006,5.006,0,0,0,5-5V9A5.006,5.006,0,0,0,19,4ZM9.276,2.39A1.006,1.006,0,0,1,10.068,2h3.864a1.008,1.008,0,0,1,.792.39L15.966,4H8.034ZM22,19a3,3,0,0,1-3,3H5a3,3,0,0,1-3-3V9A3,3,0,0,1,5,6H19a3,3,0,0,1,3,3Z"
              ></path><path
                d="M12,8a6,6,0,1,0,6,6A6.006,6.006,0,0,0,12,8Zm0,10a4,4,0,1,1,4-4A4,4,0,0,1,12,18Z"
              ></path></svg
            >
          </a>
        </aside>
      </div>
      <div class="col-12 col-md-7">
        <section class="pb-8 pt-md-8 pb-md-8">
          <a
            class="d-block"
            data-bigpicture={'{"imgSrc":"' + Cofee2.src + '"}'}
            href="#"
          >
            <img data-aos="fade-left" class="img-fluid rounded" src={Cofee2.src} alt="..." />
          </a>
          <a
            class="d-block mt-7"
            data-bigpicture={'{"imgSrc":"' + Cofee1.src + '"}'}
            href="#"
          >
            <img data-aos="fade-in" class="img-fluid rounded" src={Cofee1.src} alt="..." />
          </a>
          <a
            class="d-block mt-7"
            data-bigpicture={'{"imgSrc":"' + Cofee3.src + '"}'}
            href="#"
          >
            <img data-aos="fade-in" class="img-fluid rounded" src={Cofee3.src} alt="..." />
          </a>
          <a
            class="d-block mt-7"
            data-bigpicture={'{"imgSrc":"' + Cofee4.src + '"}'}
            href="#"
          >
            <img data-aos="fade-in" class="img-fluid rounded" src={Cofee4.src} alt="..." />
          </a>
        </section>
      </div>
    </div>
  </div>

  <!-- SHAPE -->
  <div class="position-relative">
    <div class="shape shape-bottom shape-fluid-x text-light">
      <svg viewBox="0 0 2880 48" fill="none" xmlns="http://www.w3.org/2000/svg"
        ><path
          d="M0 48h2880V0h-720C1442.5 52 720 0 720 0H0v48z"
          fill="currentColor"></path></svg
      >
    </div>
  </div>

  <section class="pt-7 pb-10 py-md-10 bg-light">
    <div class="container">
      <div class="row align-items-center mb-5">
        <div class="col-12 col-md mb-2">
          <h2 class="mb-0">Conoce Nuestro Portafolio</h2>
          <p class="mb-0 text-body-secondary">
            Échale un vistazo a nuestros proyectos realizados.
          </p>
        </div>
      </div>
      <div class="row">
        <div class="col-12 col-md-6 col-lg-4 d-flex" data-aos="fade-up">
          <PhotographyCard
            title="VolksWagen"
            location="Ciudad Obregon"
            image={Cofee1.src}
          />
        </div>
        <div class="col-12 col-md-6 col-lg-4 d-flex" data-aos="fade-up">
          <PhotographyCard
            title="Cocinas Modulares"
            location="Los Mochis"
            image={Cofee1.src}
          />
        </div>
        <div class="col-12 col-md-6 col-lg-4 d-flex" data-aos="fade-up">
          <PhotographyCard title="CUCII" location="Choix" image={Cofee1.src} />
        </div>
      </div>
    </div>
  </section>

  <Footer useMap={false} />
  <WhatsApp />
</IndexLayout>
